package test;

class Sample{
	String dep = "인사과";
	String name = "백두산";
	int ea=450, price=900;
	int sales = ea*price;
	
	void output() {
		System.out.println("부서명 :"+dep+"\n이름 : "+name+"\n수량 : "+ea+"\n단가 : "+price+"\n매출액 : "+sales);
	}
}


public class Company {

	public static void main(String[] args) {
		Sample sam = new Sample();
		sam.output();
	}

}
